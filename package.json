{
  "name": "connectone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:clean": "rm -rf .next node_modules package-lock.json && npm install && next build",
    "build:vercel": "SKIP_ENV_VALIDATION=true SKIP_LINT=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run test/e2e",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "verify:products": "node scripts/verify-product-visibility.mjs",
    "prebuild": "npm run verify:products",
    "deploy": "npm run build && npx --yes firebase-tools deploy",
    "deploy:firebase": "npm run build && npx --yes firebase-tools deploy --only hosting",
    "deploy:rules": "npx --yes firebase-tools deploy --only firestore:rules"
  },
  "dependencies": {
    "@firebase/auth": "^1.11.0",
    "@firebase/firestore": "^4.9.1",
    "@firebase/storage": "^0.14.0",
    "@google-cloud/vision": "^5.3.3",
    "@hookform/resolvers": "^3.10.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/nextjs": "^10.11.0",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@tosspayments/payment-sdk": "^1.9.1",
    "@types/node": "^20.19.13",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "assemblyai": "^4.17.0",
    "aws-sdk": "^2.1692.0",
    "canvas": "^3.2.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "firebase": "^10.14.1",
    "firebase-tools": "^14.18.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.294.0",
    "next": "14.2.15",
    "next-pwa": "^5.6.0",
    "nodemailer": "^7.0.9",
    "posthog-js": "^1.262.0",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.1",
    "twilio": "^5.10.3",
    "typescript": "^5",
    "zod": "^3.25.76",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "firebase-admin": "^13.5.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "vitest": "^3.2.4"
  }
}
